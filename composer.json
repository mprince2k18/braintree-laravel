{
    "name": "mprince/braintree",
    "description": "A Braintree bridge for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Braintree",
        "Laravel Braintree"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Mohammad Prince",
        "email": "mprince2k16@gmail.com"
    }],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.77.1",
        "graham-campbell/manager": "^4.6.1",
        "braintree/braintree_php": "^6.5.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.6.2",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.11"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Braintree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Braintree\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Braintree\\BraintreeServiceProvider"
            ],
            "aliases": {
                "Braintree": "Artisanry\\Braintree\\Facades\\Braintree"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
